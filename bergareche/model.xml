<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="autolayoutconstraints" value="false"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and mandatory="true" name="TicTacToe">
			<alt abstract="true" mandatory="true" name="Icons">
				<feature name="IconsHand"/>
				<feature name="IconsBlueRed"/>
				<feature name="IconsPurple"/>
			</alt>
			<or abstract="true" mandatory="true" name="Oponent">
				<feature name="OponentComputer"/>
				<feature name="OponentFriend"/>
			</or>
			<and name="About">
				<feature name="AboutUrl"/>
			</and>
			<and name="TrackGames">
				<feature name="ConsecutiveGames"/>
			</and>
		</and>
	</struct>
	<constraints>
		<rule>
			<not>
				<var>TrackGames</var>
			</not>
		</rule>
		<rule>
			<not>
				<var>OponentComputer</var>
			</not>
		</rule>
	</constraints>
	<featureOrder userDefined="true">
		<feature name="TicTacToe"/>
		<feature name="OponentComputer"/>
		<feature name="OponentFriend"/>
		<feature name="IconsHand"/>
		<feature name="IconsBlueRed"/>
		<feature name="IconsPurple"/>
		<feature name="About"/>
		<feature name="AboutUrl"/>
		<feature name="TrackGames"/>
		<feature name="ConsecutiveGames"/>
	</featureOrder>
</featureModel>
